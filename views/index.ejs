<%- include('./partials/header') %>
<h1><%= title %></h1>
<p>The list of Flights</p>
<table>
  <thead>
    <tr>
      <th>Airline</th>
      <th>Airport</th>
      <th>Flight Num</th>
      <th>Departure</th>
    </tr>
  </thead>
  <tbody>
    <% flights.forEach((flight) => {%>
    <tr>
      <td><%= flight.airline %></td>
      <td><%= flight.airport %></td>
      <td><%= flight.flightNo %></td>
      <td><%= flight.departs %></td>
    </tr>
    <% }); %>
  </tbody>
</table>
<%- include('./partials/footer') %>
